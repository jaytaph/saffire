import io;

io.println(  1.hex()   );
io.println(  16.hex()   );
io.println(  256.hex()   );
io.println(  100.hex()   );
io.println(  42.hex()   );
io.println(  255.hex()   );

io.println(  1.dec()   );
io.println(  16.dec()   );
io.println(  256.dec()   );
io.println(  100.dec()   );
io.println(  42.dec()   );

io.println(  1.bin()   );
io.println(  16.bin()   );
io.println(  21.bin()   );
io.println(  256.bin()   );
io.println(  100.bin()   );
io.println(  42.bin()   );

// These are decimals!
io.println(  11111110.bin()   );
io.println(  11111110.oct()   );
io.println(  11111110.dec()   );
io.println(  11111110.hex()   );
