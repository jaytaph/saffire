author: Joshua Thijssen <joshua@saffire-lang.org>

**********
import io;

foo = 1;
switch (foo) {
    case 1 :
        io.print("f1\n");
        break;
    case 2 :
        io.print("f2\n");
        break;
    case 3 :
        io.print("f3\n");
    case 4 :
        io.print("f4\n");
        break;
    default :
        io.print("fd\n");
        break;
}
